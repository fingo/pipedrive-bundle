{
    "name": "jgendera/pipedrive-bundle",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Fingo\\Bundle\\PipedriveBundle\\": "src/",
            "Fingo\\Bundle\\PipedriveBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jacek Gendera <jacek.gendera@fingo.pl>"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.0",
        "pipedrive/pipedrive": "^4.0",
        "symfony/dependency-injection": "^5.4",
        "symfony/config": "^5.4",
        "symfony/http-kernel": "5.4.x-dev",
        "symfony/yaml": "5.4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.4.x-dev",
        "symfony/css-selector": "5.4.x-dev",
        "symfony/phpunit-bridge": "6.3.x-dev"
    }
}
